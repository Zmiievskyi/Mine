# Production Environment Variables
# Copy this file to .env and fill in the values

# App
APP_PORT=80
CORS_ORIGINS=https://your-domain.com
FRONTEND_URL=https://your-domain.com

# Database
DB_USERNAME=minegnk
DB_PASSWORD=change-this-secure-password
DB_DATABASE=minegnk

# JWT (REQUIRED - generate with: openssl rand -base64 32)
JWT_SECRET=your-super-secret-jwt-key-at-least-32-chars
JWT_EXPIRES_IN=7d

# Gonka Network
HYPERFUSION_URL=https://tracker.gonka.hyperfusion.io
GONKA_API_NODES=http://node1.gonka.ai:8000,http://node2.gonka.ai:8000,http://node3.gonka.ai:8000
GONKA_CACHE_TTL=120

# Google OAuth (optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_CALLBACK_URL=https://your-domain.com/api/auth/google/callback

# GitHub OAuth (optional)
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_CALLBACK_URL=https://your-domain.com/api/auth/github/callback
